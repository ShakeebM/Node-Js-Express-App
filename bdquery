select * from (select * from oneshot where Parameter='Voltage Adc' ORDER BY MeasTime limit 1) union (select * from( select * from oneshot where Parameter='Current Bal' ORDER BY MeasTime limit 1) union select * from( select * from oneshot where Parameter='IC Temperature' ORDER BY MeasTime limit 1));


